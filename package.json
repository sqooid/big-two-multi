{
  "name": "big-two-multi",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "clean-server": "rimraf ./dist/server/ ./dist/interfaces",
    "dev-server": "nodemon",
    "debug-server": "nodemon --inspect",
    "build-server": "npm run clean-server && tsc --project tsconfig.server.json && tscpaths -p tsconfig.server.json -s ./src -o ./dist"
  },
  "dependencies": {
    "@sqooid/big-two": "^1.0.5",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "flipping": "^1.1.0",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "vue": "^3.2.25",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/express": "^4.17.13",
    "@types/mocha": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "naive-ui": "^2.24.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "rollup": "^2.66.0",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.4.4",
    "vfonts": "^0.0.3",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
